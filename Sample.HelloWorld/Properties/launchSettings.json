        "ASPNETCORE_ENVIRONMENT": "Development",

        //Message Settings
        "MessageSettings__MessageToReturn": "Hello World",

        //Data Store Settings
        "DataStoreSettings__FileDataStoreKeys": "1,2,3",
        "DataStoreSettings__DatabaseDataStoreKeys": "4,5,6" 

      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}